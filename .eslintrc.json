{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": [ "react"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/no-unknown-property": "error",
    "react/no-invalid-html-attribute": "error"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}